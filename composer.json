{
    "name": "juhlinus/depictr",
    "description": "A middleware for rendering static pages when crawled by search engines.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Linus Juhlin",
            "email": "linus.juhlin@protonmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "php-webdriver/webdriver": "^1.8"
    },
    "require-dev": {
        "orchestra/testbench": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Depictr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Depictr\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Depictr\\ServiceProvider"
            ]
        }
    }
}
